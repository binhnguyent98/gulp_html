{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/logs": true,
    "**/package-lock.json": true
  },
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  }
}
